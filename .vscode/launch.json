    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch Package",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${fileDirname}"
        },
        {
            "name": "Attach Overmoderatorimport {} from '@google/gemini-cli';
",
            "type": "node",
            "request": "attach",
            "port": 9230,
            "restart": false,
            "timeout": 30000,
            "skipFiles": ["<node_internals>/**"],
            "cwd": "${workspaceFolder}/packages/overmoderator"
        }
    ]
}